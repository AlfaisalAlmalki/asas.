#. module: odoo_whatsapp_integration
#: model:mail.template,body_html:odoo_whatsapp_integration.whatsapp_invoice_template
msgid ""
"\n"
"            <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, "
"Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <div>\n"
"                    Dear *${object.partner_id.name}*\n"
"                </div>\n"
"                <div>\n"
"                    Here is your invoice *${object.name}* with amount "
"*${format_amount(object.amount_total, object.currency_id)}*\n"
"                    from ${object.company_id.name}.\n"
"                </div>\n"
"                <div>\n"
"                    % if object.invoice_payment_state == 'paid':\n"
"                        This invoice is already paid. Amount due is "
"*${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % else:\n"
"                        Please remit payment at your earliest convenience. "
"Amount due is *${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % endif\n"
"                </div>\n"
"                The invoice date and time is \"${object.invoice_date}\"\n"
"                <div>\n"
"                    Your order details are as follows:\n"
"                    <br>\n"
"                    % for each in object:\n"
"                        % for id in each.invoice_line_ids:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.quantity}* <br>\n"
"                            *Price: ${id.price_unit}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"                If you have any questions, please feel free to contact us.\n"
"        \n"
"        "
msgstr ""
"\n"
"            <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, "
"Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <div>\n"
"                    عزيزي*${object.partner_id.name}*\n"
"                </div>\n"
"                <div>\n"
"                    هنا الفاتورة الخاصة بك *${object.name}* مع المبلغ "
"*${format_amount(object.amount_total, object.currency_id)}*\n"
"                    من ${object.company_id.name}.\n"
"                </div>\n"
"                <div>\n"
"                    % if object.invoice_payment_state == 'paid':\n"
"                        هذه الفاتورة تم دفعها مسبقا. المبلغ المتبقي "
"*${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % else:\n"
"                       يرجى تحويل المبلغ في أقرب وقت ممكن. المبلغ المستحق\n"
"*${format_amount(object.amount_residual,object.currency_id)}*\n"
"                    % endif\n"
"                </div>\n"
"                تاريخ ووقت الفاتورة هو \"${object.invoice_date}\"\n"
"                <div>\n"
"                    في التالي بيانات طلبك:\n"
"                    <br>\n"
"                    % for each in object:\n"
"                        % for id in each.invoice_line_ids:\n"
"                            *المنتج: ${id.product_id.name}* <br>\n"
"                            *الكمية: ${id.quantity}* <br>\n"
"                            *السعر: ${id.price_unit}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"                إذا كان لديك أي أسئلة ، فلا تتردد في الاتصال بنا.\n"
"        \n"
"        "

#. module: odoo_whatsapp_integration
#: model:mail.template,body_html:odoo_whatsapp_integration.whatsapp_purchase_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"             <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"             <div>\n"
"                Hello *${object.partner_id.name or ''}*,\n"
"             </div>\n"
"            % if object.state == 'draft' or object.state == 'sent':\n"
"                This is about Request For Quotation (RFQ) *${object.name}* "
"with amount *${format_amount(object.amount_total, object.currency_id)}*\n"
"                <div>\n"
"                    Quotation details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_qty}* <br>\n"
"                            *Price: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % else:\n"
"                <div>\n"
"                    Your Purchase Order Number *${object.name}* with amount "
"*${format_amount(object.amount_total, object.currency_id)}* is Confirmed.\n"
"                </div>\n"
"                <div>\n"
"                    Order confirmation date and time is \"${object."
"date_approve}\"\n"
"                </div>\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_qty}* <br>\n"
"                            *Price: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % endif\n"
"            <div>\n"
"                We are expecting receipt of products on or before scheduled "
"date <br/>\n"
"                If you have any questions, please feel free to contact us.\n"
"            </div>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"             <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"             <div>\n"
"                اهلا *${object.partner_id.name or ''}*,\n"
"             </div>\n"
"            % if object.state == 'draft' or object.state == 'sent':\n"
"                هذا حول طلب عرض أسعار (RFQ) *${object.name}* مع المبلغ "
"*${format_amount(object.amount_total, object.currency_id)}*\n"
"                <div>\n"
"                    تفاصيل عرض السعر في الاسفل: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *المنتج: ${id.product_id.name}* <br>\n"
"                            *الكمية: ${id.product_qty}* <br>\n"
"                            *السعر: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % else:\n"
"                <div>\n"
"                    رقم طلب الشراء الخاص بك *${object.name}* مع المبلغ "
"*${format_amount(object.amount_total, object.currency_id)}* تم الموافقة عليه."
"\n"
"                </div>\n"
"                <div>\n"
"                    تاريخ ووقت الموافقة على الطلب \"${object.date_approve}"
"\"\n"
"                </div>\n"
"                <div>\n"
"                    تفاصيل الطلب في الاسفل: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.order_line:\n"
"                            *المنتج: ${id.product_id.name}* <br>\n"
"                            *الكمية: ${id.product_qty}* <br>\n"
"                            *السعر: ${id.price_subtotal}*<br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"            % endif\n"
"            <div>\n"
"                نتوقع استلام المنتجات في الموعد المحدد أو قبله <br/>\n"
"                إذا كان لديك أي أسئلة ، فلا تتردد في الاتصال بنا.\n"
"            </div>\n"
"        \n"
"        "

